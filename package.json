{
  "name": "lecturebot",
  "version": "1.1.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node --unhandled-rejections=warn app.js",
    "dev": "nodemon --unhandled-rejections=warn app.js",
    "db:seed": "node db/seeds/index.js",
    "lint:fix": "eslint --fix './**/*.js'",
    "lint:quiet": "eslint --quiet './**/*.js'",
    "lint": "eslint './**/*.js'",
    "test": "jest --setupFiles dotenv/config --detectOpenHandles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": ["text-summary", "html"],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LambdaSchool/lecturebot.git"
  },
  "keywords": [],
  "author": "Dan Levy @justsml",
  "license": "MIT",
  "dependencies": {
    "analytics-node": "^3.4.0-beta.1",
    "botbuilder-adapter-slack": "^1.0.7",
    "botkit": "^4.6.1",
    "date-fns": "^2.8.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "mongoose": "^5.7.12"
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.4",
    "@types/jest": "^24.0.23",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "functional-promises": "^1.7.98",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.1"
  }
}
